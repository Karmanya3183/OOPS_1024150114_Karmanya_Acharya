#include <iostream>
using namespace std;

// First namespace
namespace SectionA {
    int value = 10;

    void display() {
        cout << "SectionA value = " << value << endl;
    }
}

// Second namespace
namespace SectionB {
    int value = 20;

    void display() {
        cout << "SectionB value = " << value << endl;
    }
}

int main() {
    // Accessing members using scope resolution
    SectionA::display();
    SectionB::display();

    cout << "SectionA value accessed directly = " << SectionA::value << endl;
    cout << "SectionB value accessed directly = " << SectionB::value << endl;

    return 0;
}
