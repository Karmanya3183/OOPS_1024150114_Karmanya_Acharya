#include <iostream>
using namespace std;

int main() {
    int days;
    cin >> days;

    if (days <= 5)
        cout << days * 0.5;
    else if (days <= 10)
        cout << (5 * 0.5) + (days - 5) * 1;
    else if (days <= 30)
        cout << (5 * 0.5) + (5 * 1) + (days - 10) * 5;
    else
        cout << "Membership Cancelled";

    return 0;
}
